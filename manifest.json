{
	"name": "A",
	"version": "1.0",
	"manifest_version": 2,
	"chrome_url_overrides":{
	"newtab": "newtab.html"
	},
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
	"background": {
		"scripts":["background.js"]
	},
	"browser_action":{
		"default_popup": "popup.html",
		"default_title": "popup"
	},
	"permissions":
		[   "webRequest",
			"*://*/*",
			"webRequestBlocking",
			"tabs"
		  ]
}
